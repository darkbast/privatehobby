<!DOCTYPE html>
<html>
<head>
  <meta charset='UTF-8'>
  <meta name="viewport" content="initial-scale=1">
  <script type='text/javascript' src='/disp/lib/js/jquery-3.3.1.min.js'></script>
  <script type='text/javascript' src='/disp/js/login/login.js'></script>
  <title>로그인</title>

<!--    <link rel='stylesheet' href='/disp/lib/css/bootstrap.css'>-->
<!--
    <script type='text/javascript' src='/disp/lib/js/bootstrap.js'></script>
    <script type='text/javascript' src='/disp/js/login/login.js'></script>
-->
</head>
<body>
  <% include ../layout/menu_login %>
  <div style="margin: 10px">
      <form name="login_form" action="/login/check" method="post">
          <div class="form-group">
              <label for="user_id">아이디</label>
              <input id="user_id" name="user_id" class="form-control" type="text" placeholder="아이디를 입력하세요.">        
          </div>
          <div class="form-group">
              <label for="user_pwd">패스워드</label>
              <input id="user_pwd" name="user_pwd" class="form-control" type="password" placeholder="패스워드를 입력하세요.">        
          </div>
          <button id="login" type="button" class="btn btn-primary">로그인</button>
          <a href="/login/login_regist" class="btn btn-primary">계정등록</a>

      </form>
      <div>
          <label id="status" style="color: #FF0000"></label>
      </div>
  </div>
</body>
</html>